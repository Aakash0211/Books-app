{
  "expo": {
    "name": "Book-app",
    "slug": "Books-app",
    "privacy": "public",
    "platforms":[
     "ios",
     "android"
    ],
    "version": "1.0.3",
    "icon": "./assets/icon1.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier":"com.Aakash0211.books-app",
      "buildNumber":"1.1.0",
      "supportsTablet": true
    },
    "android": {
      "package":"com.Aakash0211_books_app",
      "versionCode":3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions":[
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "googleServicesFile": "./google-services.json"
    }
  }
}
